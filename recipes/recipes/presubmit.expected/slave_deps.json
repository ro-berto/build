[
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_REPO[depot_tools]/gerrit_client.py",
      "changes",
      "--host",
      "https://chromium-review.googlesource.com",
      "--json_file",
      "/path/to/tmp/json",
      "--limit",
      "1",
      "-p",
      "change=456789",
      "-o",
      "ALL_REVISIONS",
      "-o",
      "DOWNLOAD_COMMANDS"
    ],
    "env": {
      "PATH": "<PATH>:RECIPE_REPO[depot_tools]"
    },
    "infra_step": true,
    "name": "gerrit fetch current CL info",
    "timeout": 60,
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@[@@@",
      "@@@STEP_LOG_LINE@json.output@  {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"branch\": \"main\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"owner\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"name\": \"John Doe\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"184ebe53805e102605d11f6b143486d15c23a09c\": {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"_number\": \"12\", @@@",
      "@@@STEP_LOG_LINE@json.output@        \"ref\": \"refs/changes/89/456789/12\"@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@]@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "ensure-directory",
      "--mode",
      "0777",
      "[CACHE]/builder/slave_deps_presubmit"
    ],
    "infra_step": true,
    "name": "ensure builder cache dir"
  },
  {
    "cmd": [
      "python3",
      "-u",
      "RECIPE_MODULE[depot_tools::bot_update]/resources/bot_update.py",
      "--spec-path",
      "cache_dir = '[CACHE]/git'\nsolutions = [{'deps_file': '.DEPS.git', 'managed': True, 'name': 'slave.DEPS', 'url': 'https://chrome-internal.googlesource.com/chrome/tools/build/slave.DEPS.git'}]",
      "--patch_root",
      "slave.DEPS",
      "--revision_mapping_file",
      "{\"got_revision\": \"slave.DEPS\"}",
      "--git-cache-dir",
      "[CACHE]/git",
      "--cleanup-dir",
      "[CLEANUP]/bot_update",
      "--output_json",
      "/path/to/tmp/json",
      "--patch_ref",
      "https://chromium.googlesource.com/slave_deps@refs/heads/main:refs/changes/89/456789/12",
      "--revision",
      "slave.DEPS@refs/heads/main",
      "--refs",
      "refs/heads/main",
      "--no_fetch_tags"
    ],
    "cwd": "[CACHE]/builder/slave_deps_presubmit",
    "env": {
      "DEPOT_TOOLS_COLLECT_METRICS": "0",
      "GIT_HTTP_LOW_SPEED_LIMIT": "102400",
      "GIT_HTTP_LOW_SPEED_TIME": "1800"
    },
    "env_suffixes": {
      "DEPOT_TOOLS_UPDATE": [
        "0"
      ],
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "bot_update",
    "timeout": 3600,
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"fixed_revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"slave.DEPS\": \"HEAD\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"slave.DEPS\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"repository\": \"https://fake.org/slave.DEPS.git\", @@@",
      "@@@STEP_LOG_LINE@json.output@      \"revision\": \"cf7b8d09b4eb1791bceecb9b7d0a8e4799175a4e\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"slave.DEPS\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision\": \"cf7b8d09b4eb1791bceecb9b7d0a8e4799175a4e\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision_cp\": \"refs/heads/main@{#80745}\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"slave.DEPS\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"source_manifest\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"directories\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"slave.DEPS\": {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"git_checkout\": {@@@",
      "@@@STEP_LOG_LINE@json.output@          \"repo_url\": \"https://fake.org/slave.DEPS.git\", @@@",
      "@@@STEP_LOG_LINE@json.output@          \"revision\": \"cf7b8d09b4eb1791bceecb9b7d0a8e4799175a4e\"@@@",
      "@@@STEP_LOG_LINE@json.output@        }@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    }, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"version\": 0@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@\"cf7b8d09b4eb1791bceecb9b7d0a8e4799175a4e\"@@@",
      "@@@SET_BUILD_PROPERTY@got_revision_cp@\"refs/heads/main@{#80745}\"@@@"
    ]
  },
  {
    "cmd": [
      "git",
      "-c",
      "user.email=commit-bot@chromium.org",
      "-c",
      "user.name=The Commit Bot",
      "commit",
      "-a",
      "-m",
      "Committed patch"
    ],
    "cwd": "[CACHE]/builder/slave_deps_presubmit/slave.DEPS",
    "name": "commit-git-patch"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_REPO[depot_tools]/presubmit_support.py",
      "--issue",
      "456789",
      "--patchset",
      "12",
      "--gerrit_url",
      "https://chromium-review.googlesource.com",
      "--gerrit_project",
      "slave_deps",
      "--gerrit_branch",
      "refs/heads/main",
      "--gerrit_fetch",
      "--root",
      "[CACHE]/builder/slave_deps_presubmit/slave.DEPS",
      "--commit",
      "--verbose",
      "--verbose",
      "--skip_canned",
      "CheckTreeIsOpen",
      "--upstream",
      "cf7b8d09b4eb1791bceecb9b7d0a8e4799175a4e",
      "--json_output",
      "/path/to/tmp/json"
    ],
    "cwd": "[CACHE]/builder/slave_deps_presubmit",
    "env_suffixes": {
      "DEPOT_TOOLS_UPDATE": [
        "0"
      ],
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "name": "presubmit",
    "timeout": 654,
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"errors\": [], @@@",
      "@@@STEP_LOG_LINE@json.output@  \"notifications\": [], @@@",
      "@@@STEP_LOG_LINE@json.output@  \"warnings\": []@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "vpython3",
      "RECIPE_REPO[depot_tools]/presubmit_support.py",
      "--issue",
      "456789",
      "--patchset",
      "12",
      "--gerrit_url",
      "https://chromium-review.googlesource.com",
      "--gerrit_project",
      "slave_deps",
      "--gerrit_branch",
      "refs/heads/main",
      "--gerrit_fetch",
      "--root",
      "[CACHE]/builder/slave_deps_presubmit/slave.DEPS",
      "--commit",
      "--verbose",
      "--verbose",
      "--skip_canned",
      "CheckTreeIsOpen",
      "--upstream",
      "cf7b8d09b4eb1791bceecb9b7d0a8e4799175a4e",
      "--json_output",
      "/path/to/tmp/json"
    ],
    "cwd": "[CACHE]/builder/slave_deps_presubmit",
    "env_suffixes": {
      "DEPOT_TOOLS_UPDATE": [
        "0"
      ],
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "name": "presubmit py3",
    "timeout": 654,
    "~followup_annotations": [
      "@@@STEP_LOG_END@json.output (invalid)@@@",
      "@@@STEP_LOG_LINE@json.output (exception)@No JSON object could be decoded@@@",
      "@@@STEP_LOG_END@json.output (exception)@@@"
    ]
  },
  {
    "name": "$result"
  }
]