[
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_REPO[depot_tools]\\gerrit_client.py",
      "changes",
      "--host",
      "https://chromium-review.googlesource.com",
      "--json_file",
      "/path/to/tmp/json",
      "--limit",
      "1",
      "-p",
      "change=456789",
      "-o",
      "ALL_REVISIONS",
      "-o",
      "DOWNLOAD_COMMANDS"
    ],
    "cwd": "[CACHE]\\builder",
    "env": {
      "PATH": "<PATH>;RECIPE_REPO[depot_tools]"
    },
    "infra_step": true,
    "name": "gerrit fetch current CL info",
    "timeout": 60,
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@[@@@",
      "@@@STEP_LOG_LINE@json.output@  {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"branch\": \"master\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"owner\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"name\": \"John Doe\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"184ebe53805e102605d11f6b143486d15c23a09c\": {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"_number\": \"12\", @@@",
      "@@@STEP_LOG_LINE@json.output@        \"ref\": \"refs/changes/89/456789/12\"@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@]@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::bot_update]\\resources\\bot_update.py",
      "--spec-path",
      "cache_dir = '[CACHE]\\\\git'\nsolutions = [{'deps_file': 'DEPS', 'managed': True, 'name': 'src', 'url': 'https://chromium.googlesource.com/libyuv/libyuv'}]",
      "--patch_root",
      "src",
      "--revision_mapping_file",
      "{\"got_revision\": \"src\"}",
      "--git-cache-dir",
      "[CACHE]\\git",
      "--cleanup-dir",
      "[CLEANUP]\\bot_update",
      "--output_json",
      "/path/to/tmp/json",
      "--patch_ref",
      "https://chromium.googlesource.com/libyuv/libyuv@refs/heads/master:refs/changes/89/456789/12",
      "--revision",
      "src@aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa",
      "--refs",
      "refs/heads/main"
    ],
    "cwd": "[CACHE]\\builder",
    "env": {
      "DEPOT_TOOLS_REPORT_BUILD": "libyuv/try/win_x64_rel/8945511751514863184",
      "GIT_HTTP_LOW_SPEED_LIMIT": "102400",
      "GIT_HTTP_LOW_SPEED_TIME": "1800"
    },
    "env_suffixes": {
      "DEPOT_TOOLS_UPDATE": [
        "0"
      ],
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"fixed_revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"src\": \"aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"src\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"repository\": \"https://fake.org/src.git\", @@@",
      "@@@STEP_LOG_LINE@json.output@      \"revision\": \"aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"src\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision\": \"aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision_cp\": \"refs/heads/master@{#170242}\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"src\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"source_manifest\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"directories\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"src\": {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"git_checkout\": {@@@",
      "@@@STEP_LOG_LINE@json.output@          \"repo_url\": \"https://fake.org/src.git\", @@@",
      "@@@STEP_LOG_LINE@json.output@          \"revision\": \"aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa\"@@@",
      "@@@STEP_LOG_LINE@json.output@        }@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    }, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"version\": 0@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@\"aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa\"@@@",
      "@@@SET_BUILD_PROPERTY@got_revision_cp@\"refs/heads/master@{#170242}\"@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_REPO[depot_tools]\\gclient.py",
      "runhooks"
    ],
    "cwd": "[CACHE]\\builder\\src",
    "env": {
      "CHROME_HEADLESS": "1",
      "DEPOT_TOOLS_REPORT_BUILD": "libyuv/try/win_x64_rel/8945511751514863184"
    },
    "env_suffixes": {
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_REPO[depot_tools]\\gn.py",
      "--root=[CACHE]\\builder\\src",
      "gen",
      "//out/Release_x64",
      "--args=is_clang=false dcheck_always_on=true is_debug=false target_cpu=\"x64\""
    ],
    "cwd": "[CACHE]\\builder\\src",
    "env": {
      "CHROME_HEADLESS": "1"
    },
    "name": "gn"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[build::chromium]\\resources\\ninja_wrapper.py",
      "--ninja_info_output",
      "/path/to/tmp/json",
      "--failure_output",
      "/path/to/tmp/",
      "--",
      "RECIPE_REPO[depot_tools]\\ninja.exe",
      "-w",
      "dupbuild=err",
      "-C",
      "[CACHE]\\builder\\src\\out\\Release_x64"
    ],
    "cwd": "[CACHE]\\builder\\src",
    "env": {
      "CHROME_HEADLESS": "1"
    },
    "name": "compile"
  },
  {
    "cmd": [
      "RECIPE_REPO[depot_tools]\\ninja.exe",
      "-w",
      "dupbuild=err",
      "-C",
      "[CACHE]\\builder\\src\\out\\Release_x64",
      "-d",
      "explain",
      "-n"
    ],
    "cwd": "[CACHE]\\builder\\src",
    "env": {
      "CHROME_HEADLESS": "1"
    },
    "name": "compile confirm no-op"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_REPO[build]\\scripts\\tools\\runit.py",
      "--show-path",
      "--",
      "python",
      "RECIPE_REPO[build]\\recipes\\upload_goma_logs.py",
      "--gsutil-py-path",
      "RECIPE_REPO[depot_tools]\\gsutil.py",
      "--skip-sendgomatsmon",
      "--ninja-log-outdir",
      "[CACHE]\\builder\\src\\out\\Release_x64",
      "--ninja-log-command-file",
      "[\"RECIPE_REPO[depot_tools]\\\\ninja.exe\", \"-w\", \"dupbuild=err\", \"-C\", \"[CACHE]\\\\builder\\\\src\\\\out\\\\Release_x64\"]",
      "--build-exit-status",
      "0",
      "--ninja-log-compiler",
      "unknown"
    ],
    "cwd": "[CACHE]\\builder\\src",
    "env": {
      "CHROME_HEADLESS": "1"
    },
    "name": "upload_ninja_log"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_REPO[build]\\scripts\\tools\\runit.py",
      "--show-path",
      "--with-third-party-lib",
      "--",
      "python",
      "RECIPE_REPO[build]\\recipes\\runtest.py",
      "--target",
      "Release_x64",
      "--build-properties",
      "{\"BUILD_CONFIG\": \"Release\", \"bot_id\": \"bot_id\", \"builder_group\": \"tryserver.libyuv\", \"buildername\": \"win_x64_rel\", \"buildnumber\": 1337, \"recipe\": \"libyuv/libyuv\", \"slavename\": \"bot_id\"}",
      "--step-name=libyuv_unittest",
      "--builder-name=win_x64_rel",
      "--slave-name=bot_id",
      "--build-number=1337",
      "libyuv_unittest.exe",
      "--undefok=no-sandbox"
    ],
    "cwd": "[CACHE]\\builder",
    "env": {
      "BUILD_SLAVE_UTILS_GSUTIL_PY_PATH": "RECIPE_REPO[depot_tools]\\gsutil.py",
      "CHROME_HEADLESS": "1"
    },
    "name": "libyuv_unittest"
  },
  {
    "name": "$result"
  }
]