[
  {
    "cmd": [
      "uname",
      "-a"
    ],
    "env": {
      "GOCACHE": "[CACHE]/gocache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[CACHE]/gopath"
    },
    "name": "uname"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_REPO[depot_tools]/gerrit_client.py",
      "changes",
      "--host",
      "https://boringssl-review.googlesource.com",
      "--json_file",
      "/path/to/tmp/json",
      "--limit",
      "1",
      "-p",
      "change=123456",
      "-o",
      "ALL_REVISIONS",
      "-o",
      "DOWNLOAD_COMMANDS"
    ],
    "env": {
      "GOCACHE": "[CACHE]/gocache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[CACHE]/gopath",
      "PATH": "<PATH>:RECIPE_REPO[depot_tools]"
    },
    "infra_step": true,
    "name": "gerrit fetch current CL info",
    "timeout": 60,
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@[@@@",
      "@@@STEP_LOG_LINE@json.output@  {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"branch\": \"main\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"owner\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"name\": \"John Doe\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"184ebe53805e102605d11f6b143486d15c23a09c\": {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"_number\": \"7\", @@@",
      "@@@STEP_LOG_LINE@json.output@        \"ref\": \"refs/changes/56/123456/7\"@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@]@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::bot_update]/resources/bot_update.py",
      "--spec-path",
      "cache_dir = '[CACHE]/git'\nsolutions = [{'deps_file': 'util/bot/DEPS', 'managed': True, 'name': 'boringssl', 'url': 'https://boringssl.googlesource.com/boringssl.git'}]",
      "--patch_root",
      "boringssl",
      "--revision_mapping_file",
      "{}",
      "--git-cache-dir",
      "[CACHE]/git",
      "--cleanup-dir",
      "[CLEANUP]/bot_update",
      "--output_json",
      "/path/to/tmp/json",
      "--patch_ref",
      "https://boringssl.googlesource.com/boringssl@refs/heads/main:refs/changes/56/123456/7",
      "--revision",
      "boringssl@refs/heads/main",
      "--refs",
      "refs/heads/main"
    ],
    "env": {
      "DEPOT_TOOLS_REPORT_BUILD": "boringssl/try/linux/8945511751514863184",
      "GIT_HTTP_LOW_SPEED_LIMIT": "102400",
      "GIT_HTTP_LOW_SPEED_TIME": "1800",
      "GOCACHE": "[CACHE]/gocache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[CACHE]/gopath"
    },
    "env_suffixes": {
      "DEPOT_TOOLS_UPDATE": [
        "0"
      ],
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"fixed_revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"boringssl\": \"HEAD\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"boringssl\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"repository\": \"https://fake.org/boringssl.git\", @@@",
      "@@@STEP_LOG_LINE@json.output@      \"revision\": \"3eadb8277dcad90ab3dc6c8a984e15817f286492\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"boringssl\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"boringssl\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"source_manifest\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"directories\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"boringssl\": {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"git_checkout\": {@@@",
      "@@@STEP_LOG_LINE@json.output@          \"repo_url\": \"https://fake.org/boringssl.git\", @@@",
      "@@@STEP_LOG_LINE@json.output@          \"revision\": \"3eadb8277dcad90ab3dc6c8a984e15817f286492\"@@@",
      "@@@STEP_LOG_LINE@json.output@        }@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    }, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"version\": 0@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_REPO[depot_tools]/gclient.py",
      "runhooks"
    ],
    "cwd": "[START_DIR]/boringssl",
    "env": {
      "DEPOT_TOOLS_REPORT_BUILD": "boringssl/try/linux/8945511751514863184",
      "GOCACHE": "[CACHE]/gocache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[CACHE]/gopath"
    },
    "env_suffixes": {
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "rmtree",
      "[START_DIR]/boringssl/build"
    ],
    "env": {
      "GOCACHE": "[CACHE]/gocache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[CACHE]/gopath"
    },
    "infra_step": true,
    "name": "clean"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "ensure-directory",
      "--mode",
      "0777",
      "[START_DIR]/boringssl/build"
    ],
    "env": {
      "GOCACHE": "[CACHE]/gocache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[CACHE]/gopath"
    },
    "infra_step": true,
    "name": "mkdir"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/boringssl/util/bot/go/env.py",
      "[START_DIR]/boringssl/util/bot/cmake-linux64/bin/cmake",
      "-GNinja",
      "-DBORINGSSL_ALLOW_CXX_RUNTIME=1",
      "-DCMAKE_MAKE_PROGRAM=RECIPE_REPO[depot_tools]/ninja",
      "[START_DIR]/boringssl"
    ],
    "cwd": "[START_DIR]/boringssl/build",
    "env": {
      "GOCACHE": "[CACHE]/gocache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[CACHE]/gopath"
    },
    "name": "cmake"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/boringssl/util/bot/go/env.py",
      "RECIPE_REPO[depot_tools]/ninja",
      "-C",
      "[START_DIR]/boringssl/build"
    ],
    "env": {
      "GOCACHE": "[CACHE]/gocache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[CACHE]/gopath"
    },
    "name": "ninja"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/boringssl/util/bot/go/env.py",
      "go",
      "run",
      "[START_DIR]/boringssl/util/check_filenames.go"
    ],
    "cwd": "[START_DIR]/boringssl",
    "env": {
      "GOCACHE": "[CACHE]/gocache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[CACHE]/gopath"
    },
    "name": "check filenames"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/boringssl/util/bot/go/env.py",
      "go",
      "run",
      "util/all_tests.go",
      "-json-output",
      "/path/to/tmp/json"
    ],
    "cwd": "[START_DIR]/boringssl",
    "env": {
      "GOCACHE": "[CACHE]/gocache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[CACHE]/gopath"
    },
    "name": "unit tests",
    "~followup_annotations": [
      "@@@STEP_LOG_END@test_utils.test_results (invalid)@@@",
      "@@@STEP_LOG_LINE@test_utils.test_results (exception)@No JSON object could be decoded@@@",
      "@@@STEP_LOG_END@test_utils.test_results (exception)@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/boringssl/util/bot/go/env.py",
      "go",
      "test",
      "-json-output",
      "/path/to/tmp/json",
      "-pipe"
    ],
    "cwd": "[START_DIR]/boringssl/ssl/test/runner",
    "env": {
      "GOCACHE": "[CACHE]/gocache",
      "GOFLAGS": "-mod=readonly",
      "GOPATH": "[CACHE]/gopath"
    },
    "name": "ssl tests",
    "~followup_annotations": [
      "@@@STEP_LOG_END@test_utils.test_results (invalid)@@@",
      "@@@STEP_LOG_LINE@test_utils.test_results (exception)@No JSON object could be decoded@@@",
      "@@@STEP_LOG_END@test_utils.test_results (exception)@@@"
    ]
  },
  {
    "name": "$result"
  }
]