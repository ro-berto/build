[
  {
    "cmd": [],
    "name": "Roller: 'Update dependencies'",
    "~followup_annotations": [
      "@@@STEP_FAILURE@@@"
    ]
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_REPO[depot_tools]/gerrit_client.py",
      "changes",
      "--host",
      "https://review.googlesource.com",
      "--json_file",
      "/path/to/tmp/json",
      "--limit",
      "20",
      "-p",
      "project=v8/v8",
      "-p",
      "owner=autoroll@service-accounts.com",
      "-p",
      "status=open",
      "-o",
      "LABELS",
      "-o",
      "CURRENT_REVISION",
      "-o",
      "DOWNLOAD_COMMANDS"
    ],
    "env": {
      "PATH": "<PATH>:RECIPE_REPO[depot_tools]"
    },
    "env_suffixes": {
      "DEPOT_TOOLS_UPDATE": [
        "0"
      ],
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "Roller: 'Update dependencies'.gerrit Find open CLs",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@",
      "@@@STEP_LOG_LINE@json.output@[@@@",
      "@@@STEP_LOG_LINE@json.output@  {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"_number\": 123, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"project\": \"project1\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"last\": {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"_number\": 1@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    }, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"subject\": \"Update dependencies\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@]@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [],
    "name": "Roller: 'Update dependencies'.Checking CL",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@",
      "@@@STEP_TEXT@<br/>CL failed in CQ!@@@",
      "@@@STEP_LINK@project1/+/123@https://review.googlesource.com/c/project1/+/123@@@",
      "@@@STEP_FAILURE@@@"
    ]
  },
  {
    "cmd": [
      "bb",
      "ls",
      "-host",
      "cr-buildbucket.appspot.com",
      "-json",
      "-nopage",
      "-n",
      "100",
      "-fields",
      "steps.*",
      "-predicate",
      "{\"gerritChanges\": [{\"change\": \"123\", \"host\": \"review.googlesource.com\", \"patchset\": \"1\", \"project\": \"v8/v8\"}]}"
    ],
    "env_suffixes": {
      "DEPOT_TOOLS_UPDATE": [
        "0"
      ],
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "Roller: 'Update dependencies'.Checking CL.buildbucket.search",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@2@@@",
      "@@@STEP_LOG_LINE@raw_io.output_text@{\"id\": \"1\", \"status\": \"SUCCESS\"}@@@",
      "@@@STEP_LOG_LINE@raw_io.output_text@{\"id\": \"2\", \"status\": \"FAILURE\"}@@@",
      "@@@STEP_LOG_END@raw_io.output_text@@@"
    ]
  },
  {
    "name": "$result"
  }
]