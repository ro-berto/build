      "--annotate=graphing",
      "--results-url=https://chromeperf.appspot.com",
      "--perf-dashboard-id=resource_sizes (test.apk)",
      "--perf-id=test_buildername",
      "--test-type=resource_sizes (test.apk)",
      "--step-name=resource_sizes (test.apk)",
      "--builder-name=test_buildername",
      "--slave-name=test_bot_id",
      "--build-number=123",
      "--run-python-script",
      "None/build/android/resource_sizes.py",
      "[START_DIR]/test.apk"
