    "cwd": "[CACHE]/builder",
    "env": {
      "BUILD_SLAVE_UTILS_GSUTIL_PY_PATH": "RECIPE_REPO[depot_tools]/gsutil.py",
      "CHROME_HEADLESS": "1",
      "MAC_CHROMIUM_TMPDIR": "[CLEANUP]/tmp_tmp_2",
