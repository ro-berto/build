[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_REPO[build]/scripts/tools/runit.py",
      "--show-path",
      "--with-third-party-lib",
      "--",
      "python",
      "RECIPE_REPO[build]/recipes/android/archive_build.py",
      "--target",
      "Release",
      "--name",
      "build_product.zip",
      "--exclude-filter",
      "obj/*",
      "--exclude-filter",
      "gen/*",
      "--exclude-filter",
      "*/obj/*",
      "--exclude-filter",
      "*/gen/*",
      "--exclude-filter",
      "*/thinlto-cache/*",
      "--exclude-filter",
      "*.stamp",
      "--exclude-filter",
      "*.d",
      "--exclude-filter",
      "*.ninja",
      "--exclude-filter",
      ".ninja_*"
    ],
    "cwd": "None",
    "infra_step": true,
    "name": "zip_build_product"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::gsutil]/resources/gsutil_smart_retry.py",
      "--",
      "RECIPE_REPO[depot_tools]/gsutil.py",
      "----",
      "cp",
      "None/out/build_product.zip",
      "gs://test-bucket/test/path"
    ],
    "infra_step": true,
    "name": "gsutil upload_build_product",
    "~followup_annotations": [
      "@@@STEP_LINK@gsutil.upload@https://storage.cloud.google.com/test-bucket/test/path@@@"
    ]
  },
  {
    "name": "$result"
  }
]