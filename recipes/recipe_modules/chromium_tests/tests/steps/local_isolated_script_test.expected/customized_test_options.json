[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_REPO[build]/scripts/tools/runit.py",
      "--show-path",
      "--with-third-party-lib",
      "--",
      "python",
      "None/tools/swarming_client/run_isolated.py",
      "--isolated",
      "ffffffffffffffffffffffffffffffffffffffff",
      "-I",
      "https://isolateserver.appspot.com",
      "--verbose",
      "--",
      "--gtest_filter=test1:test2",
      "--gtest_repeat=20",
      "--test-launcher-retry-limit=0",
      "--gtest_also_run_disabled_tests",
      "--isolated-script-test-output",
      "/path/to/tmp/json"
    ],
    "name": "blink_web_tests",
    "~followup_annotations": [
      "@@@STEP_TEXT@<br/>Total tests: n/a<br/>@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"failures\": [], @@@",
      "@@@STEP_LOG_LINE@json.output@  \"valid\": true@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [],
    "name": "details",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@details@compile_targets: ['blink_web_tests']@@@",
      "@@@STEP_LOG_LINE@details@isolate_target: 'blink_web_tests'@@@",
      "@@@STEP_LOG_LINE@details@uses_local_devices: False@@@",
      "@@@STEP_LOG_LINE@details@uses_isolate: True@@@",
      "@@@STEP_LOG_END@details@@@"
    ]
  },
  {
    "name": "$result"
  }
]