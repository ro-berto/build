[
  {
    "cmd": [],
    "name": "symupload"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "copy",
      "[CACHE]/builder/src/build/win_toolchain.json",
      "/path/to/tmp/"
    ],
    "infra_step": true,
    "name": "symupload.find_win_toolchain",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@",
      "@@@STEP_LOG_LINE@win_toolchain.json@\"\"@@@",
      "@@@STEP_LOG_END@win_toolchain.json@@@"
    ]
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "glob",
      "[TMP_BASE]",
      "glob*.txt"
    ],
    "infra_step": true,
    "name": "symupload.expand file globs",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@",
      "@@@STEP_LOG_LINE@glob@[TMP_BASE]/glob1.txt@@@",
      "@@@STEP_LOG_LINE@glob@[TMP_BASE]/glob2.txt@@@",
      "@@@STEP_LOG_END@glob@@@"
    ]
  },
  {
    "cmd": [
      "[TMP_BASE]/symupload.exe",
      "--timeout",
      "0",
      "glob1.txt",
      "https://some.url.com"
    ],
    "name": "symupload.symupload glob1.txt",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  },
  {
    "cmd": [
      "[TMP_BASE]/symupload.exe",
      "--timeout",
      "0",
      "glob2.txt",
      "https://some.url.com"
    ],
    "name": "symupload.symupload glob2.txt",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  },
  {
    "cmd": [
      "[TMP_BASE]/symupload.exe",
      "--timeout",
      "0",
      "[TMP_BASE]/some_artifact.txt",
      "https://some.url.com"
    ],
    "name": "symupload.symupload [TMP_BASE]/some_artifact.txt",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  },
  {
    "name": "$result"
  }
]