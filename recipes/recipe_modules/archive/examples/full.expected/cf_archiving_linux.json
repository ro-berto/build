[
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "ensure-directory",
      "--mode",
      "0777",
      "[CLEANUP]/chrome_staging"
    ],
    "infra_step": true,
    "name": "create staging_dir"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "copy",
      "None/third_party/llvm-build/Release+Asserts/bin/llvm-symbolizer",
      "[START_DIR]/src/out/Release/llvm-symbolizer"
    ],
    "infra_step": true,
    "name": "Copy llvm-symbolizer"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "copy",
      "None/third_party/llvm-build/Release+Asserts/bin/sancov",
      "[START_DIR]/src/out/Release/sancov"
    ],
    "infra_step": true,
    "name": "Copy sancov"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[build::archive]/resources/filter_build_files.py",
      "--dir",
      "[START_DIR]/src/out/Release",
      "--platform",
      "linux",
      "--output",
      "/path/to/tmp/json"
    ],
    "infra_step": true,
    "name": "filter build_dir",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@[@@@",
      "@@@STEP_LOG_LINE@json.output@  \"chrome\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"icu.dat\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"lib.host\"@@@",
      "@@@STEP_LOG_LINE@json.output@]@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_REPO[build]/scripts/tools/runit.py",
      "--show-path",
      "--with-third-party-lib",
      "--",
      "python",
      "RECIPE_MODULE[build::archive]/resources/zip_archive.py",
      "[CLEANUP]/chrome_staging",
      "chrome-asan-linux-release-refs_heads_B1-123456",
      "[\"chrome\", \"icu.dat\", \"lib.host\"]",
      "[START_DIR]/src/out/Release"
    ],
    "infra_step": true,
    "name": "zipping"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_REPO[depot_tools]/gsutil.py",
      "--",
      "-h",
      "x-goog-meta-Cr-Commit-Position:refs/heads/B1@{#123456}",
      "-h",
      "x-goog-meta-Cr-Commit-Position-Number:123456",
      "-h",
      "x-goog-meta-Cr-Git-Commit:5e3250aadda2b170692f8e762d43b7e8deadbeef",
      "cp",
      "-a",
      "public-read",
      "[CLEANUP]/chrome_staging/chrome-asan-linux-release-refs_heads_B1-123456.zip",
      "gs://chromium/linux-release-subdir/chrome-asan-linux-release-refs_heads_B1-123456.zip"
    ],
    "infra_step": true,
    "name": "gsutil upload",
    "~followup_annotations": [
      "@@@STEP_LINK@gsutil.upload@https://storage.cloud.google.com/chromium/linux-release-subdir/chrome-asan-linux-release-refs_heads_B1-123456.zip@@@"
    ]
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "remove",
      "[CLEANUP]/chrome_staging/chrome-asan-linux-release-refs_heads_B1-123456.zip"
    ],
    "infra_step": true,
    "name": "chrome-asan-linux-release-refs_heads_B1-123456.zip"
  },
  {
    "name": "$result"
  }
]