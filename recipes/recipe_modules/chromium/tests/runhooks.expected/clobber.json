[
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "rmtree",
      "None/out/Release"
    ],
    "env": {
      "CHROME_HEADLESS": "1"
    },
    "infra_step": true,
    "name": "clobber"
  },
  {
    "cmd": [
      "python3",
      "-u",
      "RECIPE_REPO[depot_tools]/gclient.py",
      "runhooks"
    ],
    "cwd": "None",
    "env": {
      "CHROME_HEADLESS": "1"
    },
    "env_suffixes": {
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "name": "gclient runhooks"
  },
  {
    "name": "$result"
  }
]