[
  {
    "cmd": [
      "python",
      "RECIPE_REPO[build]/recipes/runtest.py",
      "--target",
      "Release",
      "--no-xvfb",
      "--build-properties",
      "{\"bot_id\": \"test_bot_id\", \"buildername\": \"test_buildername\", \"buildnumber\": 123, \"chromium_apply_config\": [\"memcheck\"], \"recipe\": \"chromium:tests/runtest\", \"slavename\": \"test_bot_id\"}",
      "--test-type=base_unittests",
      "--step-name=base_unittests",
      "--builder-name=test_buildername",
      "--slave-name=test_bot_id",
      "--build-number=123",
      "--revision=some_sha",
      "--pass-build-dir",
      "--pass-target",
      "--run-shell-script",
      "None/tools/valgrind/chrome_tests.sh",
      "--test",
      "base_unittests",
      "--tool",
      "memcheck"
    ],
    "cwd": "[CACHE]/builder",
    "env": {
      "BUILD_SLAVE_UTILS_GSUTIL_PY_PATH": "RECIPE_REPO[depot_tools]/gsutil.py",
      "CHROME_HEADLESS": "1"
    },
    "env_prefixes": {
      "PYTHONPATH": [
        "RECIPE_REPO[build]",
        "RECIPE_REPO[build]/recipes",
        "RECIPE_REPO[build]/scripts"
      ]
    },
    "name": "base_unittests"
  },
  {
    "name": "$result"
  }
