[
  {
    "cmd": [
      "python",
      "RECIPE_REPO[build]/recipes/runtest.py",
      "--target",
      "Release",
      "--no-xvfb",
      "--build-properties",
      "{\"annotate\": \"gtest\", \"bot_id\": \"test_bot_id\", \"buildername\": \"test_buildername\", \"buildnumber\": 123, \"recipe\": \"chromium:tests/runtest\", \"slavename\": \"test_bot_id\"}",
      "--annotate=gtest",
      "--test-type=base_unittests",
      "--test-launcher-summary-output",
      "/path/to/tmp/json",
      "--builder-name=test_buildername",
      "--slave-name=test_bot_id",
      "--build-number=123",
      "--revision=some_sha",
      "base_unittests",
      "A\nt\nE\nx\ni\nt\n*"
    ],
    "cwd": "[CACHE]/builder",
    "env": {
      "BUILD_SLAVE_UTILS_GSUTIL_PY_PATH": "RECIPE_REPO[depot_tools]/gsutil.py",
      "CHROME_HEADLESS": "1"
    },
    "env_prefixes": {
      "PYTHONPATH": [
        "RECIPE_REPO[build]",
        "RECIPE_REPO[build]/recipes",
        "RECIPE_REPO[build]/scripts"
      ]
    },
    "name": "base_unittests",
    "~followup_annotations": [
      "@@@STEP_LOG_END@json.output (invalid)@@@",
      "@@@STEP_LOG_LINE@json.output (exception)@No JSON object could be decoded@@@",
      "@@@STEP_LOG_END@json.output (exception)@@@"
    ]
  },
  {
    "name": "$result"
  }
]