{
  "master_base_class": "Master3",
  "master_classname": "Catapult",
  "master_port": 20303,
  "master_port_alt": 25303,
  "slave_port": 30303,
  "pubsub_service_account_file": "service-account-luci-milo.json",
  "pubsub_topic": "projects/luci-milo/topics/public-buildbot",
  "templates": ["../master.chromium/templates"],

  "builders": {
    "Catapult Linux": {
      "recipe": "catapult",
      "scheduler": "catapult_commits",
      "slave_pools": ["linux_slave_pool"],

      "slavebuilddir": "catapult",
    },
    "Catapult Mac": {
      "recipe": "catapult",
      "scheduler": "catapult_commits",
      "slave_pools": ["mac_slave_pool"],

      "slavebuilddir": "catapult",
    },
    "Catapult Windows": {
      "recipe": "catapult",
      "scheduler": "catapult_commits",
      "slave_pools": ["win_slave_pool"],

      "slavebuilddir": "catapult",
    },
    "Catapult Cron Linux": {
      "recipe": "catapult_cron",
      "scheduler": "catapult_cron",
      "slave_pools": ["linux_slave_pool"],
    }
  },

  "schedulers": {
    "catapult_commits": {
      "type": "git_poller",
      "git_repo_url": "https://chromium.googlesource.com/external/github.com/catapult-project/catapult.git",
    },
    "catapult_cron": {
      "type": "cron",
      "hour": 12,
      "minute": 0,
    }
  },

  "slave_pools": {
    "linux_slave_pool": {
      "slave_data": {
        "bits": 64,
        "os": "linux",
        "version": "precise",
      },
      "slaves": ["vm600-m3"],
    },
    "mac_slave_pool": {
      "slave_data": {
        "bits": 64,
        "os": "mac",
        "version": "10.9",
      },
      "slaves": ["vm113-m3"],
    },
    "win_slave_pool": {
      "slave_data": {
        "bits": 64,
        "os": "win",
        "version": "win7",
      },
      "slaves": ["vm114-m3"],
    },
  },
}
