{
  "master_base_class": "Master3",
  "master_classname": "GYP",
  "master_port": 20302,
  "master_port_alt": 25302,
  "slave_port": 30302,
  "templates": ["templates", "../master.chromium/templates"],

  "buildbucket_bucket": "master.client.gyp",
  "pubsub_service_account_file": "service-account-luci-milo.json",
  "pubsub_topic": "projects/luci-milo/topics/public-buildbot",
  "service_account_file": "service-account-gyp.json",

  "builders": {
    "linux": {
      "category": "continuous",
      "recipe": "gyp",
      "scheduler": "gyp_commits",
      "slave_pools": ["linux"],
    },
    "linux_try": {
      "category": "trybots",
      "recipe": "gyp",
      "scheduler": None,
      "slave_pools": ["linux"],
      "trybot": True,
    },
    "mac": {
      "category": "continuous",
      "recipe": "gyp",
      "scheduler": "gyp_commits",
      "slave_pools": ["mac10.9"],
    },
    "mac_try": {
      "category": "trybots",
      "recipe": "gyp",
      "scheduler": None,
      "slave_pools": ["mac10.9"],
      "trybot": True,
    },
    "win": {
      "category": "continuous",
      "recipe": "gyp",
      "scheduler": "gyp_commits",
      "slave_pools": ["win7"],
    },
    "win_try": {
      "category": "trybots",
      "recipe": "gyp",
      "scheduler": None,
      "slave_pools": ["win7"],
      "trybot": True,
    },
  },

  "schedulers": {
    "gyp_commits": {
      "type": "git_poller",
      "git_repo_url": "https://chromium.googlesource.com/external/gyp",
    },
  },

  "slave_pools": {
    "linux": {
      "slave_data": {
        "bits": 64,
        "os":  "linux",
        "version": "trusty",
      },
      "slaves": ["slave76-c3"],
    },
    "mac10.9": {
      "slave_data": {
        "bits": 64,
        "os":  "mac",
        "version": "10.9",
      },
      "slaves": ["vm605-m3"],
    },
    "win7": {
      "slave_data": {
        "bits": 64,
        "os":  "win",
        "version": "win7",
      },
      "slaves": ["vm49-m3"],
    },
  },
}
