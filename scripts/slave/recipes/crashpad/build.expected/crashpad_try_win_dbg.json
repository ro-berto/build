[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::bot_update]/resources/bot_update.py",
      "--spec-path",
      "cache_dir = '[CACHE]/git'\nsolutions = [{'custom_vars': {'pull_win_toolchain': True}, 'deps_file': '.DEPS.git', 'managed': True, 'name': 'crashpad', 'url': 'https://chromium.googlesource.com/crashpad/crashpad.git'}]\ntarget_os = ['win']",
      "--patch_root",
      "crashpad",
      "--revision_mapping_file",
      "{}",
      "--git-cache-dir",
      "[CACHE]/git",
      "--cleanup-dir",
      "[CLEANUP]/bot_update",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "crashpad@HEAD"
    ],
    "env_prefixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"fixed_revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"crashpad\": \"HEAD\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"crashpad\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"crashpad\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"source_manifest\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"directories\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"version\": 0@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[depot_tools]/gclient.py",
      "runhooks"
    ],
    "cwd": "[START_DIR]/crashpad",
    "env_suffixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "gn",
      "gen",
      "[START_DIR]/crashpad/out/Debug_x86",
      "--args=target_os=\"win\" is_debug=true win_toolchain_path=\"//third_party/win/toolchain\" target_cpu=\"x86\""
    ],
    "cwd": "[START_DIR]/crashpad",
    "name": "generate build files x86"
  },
  {
    "cmd": [
      "gn",
      "gen",
      "[START_DIR]/crashpad/out/Debug_x64",
      "--args=target_os=\"win\" is_debug=true win_toolchain_path=\"//third_party/win/toolchain\" target_cpu=\"x64\""
    ],
    "cwd": "[START_DIR]/crashpad",
    "name": "generate build files x64"
  },
  {
    "cmd": [
      "ninja",
      "-C",
      "[START_DIR]/crashpad/out/Debug_x86"
    ],
    "name": "compile with ninja x86"
  },
  {
    "cmd": [
      "ninja",
      "-C",
      "[START_DIR]/crashpad/out/Debug_x64"
    ],
    "name": "compile with ninja x64"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/crashpad/build/run_tests.py",
      "[START_DIR]/crashpad/out/Debug_x86"
    ],
    "name": "run tests",
    "timeout": 300
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/crashpad/build/run_tests.py",
      "[START_DIR]/crashpad/out/Debug_x64"
    ],
    "env": {
      "CRASHPAD_TEST_32_BIT_OUTPUT": "[START_DIR]/crashpad/out/Debug_x86"
    },
    "name": "run tests (2)",
    "timeout": 300
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "copy",
      "[START_DIR]/crashpad/build/swarming_test_spec.pyl",
      "/path/to/tmp/"
    ],
    "infra_step": true,
    "name": "read swarming_test_spec"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/crashpad/build/run_on_swarming.py",
      "[START_DIR]/crashpad/t/Debug/run_tests.isolate",
      "-d",
      "os",
      "Windows-7",
      "-d",
      "os",
      "x86-32",
      "-d",
      "os",
      "Chrome"
    ],
    "name": "run_tests_on_x86"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]