[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[build::file]/resources/fileutil.py",
      "rmcontents",
      "[START_DIR]"
    ],
    "infra_step": true,
    "name": "rmcontents everything"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::git]/resources/git_setup.py",
      "--path",
      "[START_DIR]/flutter",
      "--url",
      "https://chromium.googlesource.com/external/github.com/flutter/flutter"
    ],
    "name": "git setup"
  },
  {
    "cmd": [
      "git",
      "retry",
      "fetch",
      "origin",
      "1234abcd",
      "--recurse-submodules"
    ],
    "cwd": "[START_DIR]/flutter",
    "env": {
      "PATH": "RECIPE_PACKAGE_REPO[depot_tools]:%(PATH)s"
    },
    "infra_step": true,
    "name": "git fetch"
  },
  {
    "cmd": [
      "git",
      "checkout",
      "-f",
      "FETCH_HEAD"
    ],
    "cwd": "[START_DIR]/flutter",
    "infra_step": true,
    "name": "git checkout"
  },
  {
    "cmd": [
      "git",
      "rev-parse",
      "HEAD"
    ],
    "cwd": "[START_DIR]/flutter",
    "infra_step": true,
    "name": "read revision",
    "stdout": "/path/to/tmp/",
    "~followup_annotations": [
      "@@@STEP_TEXT@<br/>checked out 'deadbeef'<br/>@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@\"deadbeef\"@@@"
    ]
  },
  {
    "cmd": [
      "git",
      "clean",
      "-f",
      "-d",
      "-x"
    ],
    "cwd": "[START_DIR]/flutter",
    "infra_step": true,
    "name": "git clean"
  },
  {
    "cmd": [
      "git",
      "submodule",
      "sync"
    ],
    "cwd": "[START_DIR]/flutter",
    "infra_step": true,
    "name": "submodule sync"
  },
  {
    "cmd": [
      "git",
      "submodule",
      "update",
      "--init",
      "--recursive"
    ],
    "cwd": "[START_DIR]/flutter",
    "infra_step": true,
    "name": "submodule update"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/flutter/dev/bots/download_android_tools.py",
      "-t",
      "sdk"
    ],
    "name": "download android tools"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::url]/resources/pycurl.py",
      "https://services.gradle.org/distributions/gradle-2.14.1-bin.zip",
      "--outfile",
      "[START_DIR]/flutter/dev/bots/gradle-2.14.1-bin.zip"
    ],
    "name": "download gradle"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[build::zip]/resources/unzip.py"
    ],
    "name": "unzip gradle",
    "stdin": "{\"output\": \"[START_DIR]/flutter/dev/bots/gradle\", \"quiet\": false, \"zip_file\": \"[START_DIR]/flutter/dev/bots/gradle-2.14.1-bin.zip\"}"
  },
  {
    "cmd": [
      "sh",
      "-c",
      "echo y | [START_DIR]/flutter/dev/bots/android_tools/sdk/tools/android update sdk --no-ui --all --filter build-tools-25.0.2,android-25,extra-android-m2repository"
    ],
    "name": "update android tools"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[build]/scripts/slave/ios/find_xcode.py",
      "--json-file",
      "/path/to/tmp/json"
    ],
    "env": {
      "ANDROID_HOME": "[START_DIR]/flutter/dev/bots/android_tools",
      "PATH": "[START_DIR]/flutter/bin:[START_DIR]/flutter/bin/cache/dart-sdk/bin:[START_DIR]/flutter/dev/bots/gradle/gradle-2.14.1/bin:%(PATH)s",
      "PUB_CACHE": "[START_DIR]/.pub-cache"
    },
    "name": "enumerate_xcode_installations",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"installations\": {}@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "name": "$result",
    "reason": "Xcode version 7 or above not found",
    "recipe_result": null,
    "status_code": 1
  }
]