      "RECIPE_PACKAGE_REPO[depot_tools]/gsutil.py",
      "----",
      "cp",
      "[START_DIR]/flutter/packages/flutter/coverage/lcov.info",
      "gs://flutter_infra/flutter/experimental/coverage/lcov.info"
    ],
    "cwd": "[START_DIR]/flutter",
    "env": {
      "ANDROID_HOME": "[START_DIR]/flutter/dev/bots/android_tools",
      "DEPOT_TOOLS": "RECIPE_PACKAGE_REPO[depot_tools]",
      "PATH": "[START_DIR]/flutter/bin:[START_DIR]/flutter/bin/cache/dart-sdk/bin:[START_DIR]/flutter/dev/bots/gradle/gradle-2.14.1/bin:<PATH>",
      "PUB_CACHE": "[START_DIR]/flutter/.pub-cache"
    },
    "infra_step": true,
    "name": "gsutil upload coverage data",
    "~followup_annotations": [
      "@@@STEP_LINK@lcov.info@https://storage.cloud.google.com/flutter_infra/flutter/experimental/coverage/lcov.info@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "ensure-directory",
      "--mode",
      "0777",
      "[START_DIR]/gems"
    ],
    "cwd": "[START_DIR]/flutter",
    "env": {
      "ANDROID_HOME": "[START_DIR]/flutter/dev/bots/android_tools",
      "DEPOT_TOOLS": "RECIPE_PACKAGE_REPO[depot_tools]",
      "PATH": "[START_DIR]/flutter/bin:[START_DIR]/flutter/bin/cache/dart-sdk/bin:[START_DIR]/flutter/dev/bots/gradle/gradle-2.14.1/bin:<PATH>",
      "PUB_CACHE": "[START_DIR]/flutter/.pub-cache"
    },
    "infra_step": true,
    "name": "mkdir gems"
  },
  {
    "cmd": [
      "gem",
      "install",
      "coveralls-lcov:1.5.1",
      "--install-dir",
      "."
    ],
    "cwd": "[START_DIR]/gems",
    "env": {
      "ANDROID_HOME": "[START_DIR]/flutter/dev/bots/android_tools",
      "DEPOT_TOOLS": "RECIPE_PACKAGE_REPO[depot_tools]",
      "PATH": "[START_DIR]/flutter/bin:[START_DIR]/flutter/bin/cache/dart-sdk/bin:[START_DIR]/flutter/dev/bots/gradle/gradle-2.14.1/bin:<PATH>",
      "PUB_CACHE": "[START_DIR]/flutter/.pub-cache"
    },
    "name": "install coveralls-lcov"
  },
  {
    "cmd": [],
    "cwd": "[START_DIR]/flutter",
    "env": {
      "ANDROID_HOME": "[START_DIR]/flutter/dev/bots/android_tools",
      "DEPOT_TOOLS": "RECIPE_PACKAGE_REPO[depot_tools]",
      "GEM_HOME": "[START_DIR]/gems",
      "PATH": "[START_DIR]/flutter/bin:[START_DIR]/flutter/bin/cache/dart-sdk/bin:[START_DIR]/flutter/dev/bots/gradle/gradle-2.14.1/bin:<PATH>",
      "PUB_CACHE": "[START_DIR]/flutter/.pub-cache"
    },
    "env_prefixes": {
      "PATH": [
        "[START_DIR]/gems/bin"
      ]
    },
    "name": "ensure_cloudkms"
  },
  {
    "cmd": [
      "cipd",
      "ensure",
      "-root",
      "[START_DIR]/cipd/cloudkms",
      "-ensure-file",
      "infra/tools/luci/cloudkms/${platform} latest",
      "-json-output",
      "/path/to/tmp/json"
    ],
    "cwd": "[START_DIR]/flutter",
    "env": {
      "ANDROID_HOME": "[START_DIR]/flutter/dev/bots/android_tools",
      "DEPOT_TOOLS": "RECIPE_PACKAGE_REPO[depot_tools]",
      "GEM_HOME": "[START_DIR]/gems",
      "PATH": "[START_DIR]/flutter/bin:[START_DIR]/flutter/bin/cache/dart-sdk/bin:[START_DIR]/flutter/dev/bots/gradle/gradle-2.14.1/bin:<PATH>",
      "PUB_CACHE": "[START_DIR]/flutter/.pub-cache"
    },
    "env_prefixes": {
      "PATH": [
        "[START_DIR]/gems/bin"
      ]
    },
    "infra_step": true,
    "name": "ensure_cloudkms.ensure_installed",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"\": [@@@",
      "@@@STEP_LOG_LINE@json.output@      {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"instance_id\": \"resolved-instance_id-of-latest----------\", @@@",
      "@@@STEP_LOG_LINE@json.output@        \"package\": \"infra/tools/luci/cloudkms/resolved-platform\"@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    ]@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "[START_DIR]/cipd/cloudkms/cloudkms",
      "decrypt",
      "-input",
      "RECIPE[build::flutter/flutter].resources/coveralls-token.enc",
      "-output",
      "[CLEANUP]/coveralls_tmp_1",
      "flutter-infra/global/luci/coveralls"
    ],
    "cwd": "[START_DIR]/flutter",
    "env": {
      "ANDROID_HOME": "[START_DIR]/flutter/dev/bots/android_tools",
      "DEPOT_TOOLS": "RECIPE_PACKAGE_REPO[depot_tools]",
      "GEM_HOME": "[START_DIR]/gems",
      "PATH": "[START_DIR]/flutter/bin:[START_DIR]/flutter/bin/cache/dart-sdk/bin:[START_DIR]/flutter/dev/bots/gradle/gradle-2.14.1/bin:<PATH>",
      "PUB_CACHE": "[START_DIR]/flutter/.pub-cache"
    },
    "env_prefixes": {
      "PATH": [
        "[START_DIR]/gems/bin"
      ]
    },
    "name": "decrypt coveralls token"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[build]/scripts/tools/runit.py",
      "--show-path",
      "--with-third-party-lib",
      "--",
      "python",
      "RECIPE[build::flutter/flutter].resources/upload_to_coveralls.py",
      "--token-file=[CLEANUP]/coveralls_tmp_1",
      "--coverage-path=[START_DIR]/flutter/packages/flutter/coverage/lcov.info"
    ],
    "cwd": "[START_DIR]/flutter/packages/flutter",
    "env": {
      "ANDROID_HOME": "[START_DIR]/flutter/dev/bots/android_tools",
      "DEPOT_TOOLS": "RECIPE_PACKAGE_REPO[depot_tools]",
      "GEM_HOME": "[START_DIR]/gems",
      "PATH": "[START_DIR]/flutter/bin:[START_DIR]/flutter/bin/cache/dart-sdk/bin:[START_DIR]/flutter/dev/bots/gradle/gradle-2.14.1/bin:<PATH>",
      "PUB_CACHE": "[START_DIR]/flutter/.pub-cache"
    },
    "env_prefixes": {
      "PATH": [
        "[START_DIR]/gems/bin"
      ]
    },
    "name": "upload coverage data to Coveralls"
  },
  {
    "jsonResult": null,
    "name": "$result"
  }
]