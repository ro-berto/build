[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]\\resources\\fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "rmcontents",
      "[START_DIR]"
    ],
    "infra_step": true,
    "name": "everything"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::git]\\resources\\git_setup.py",
      "--path",
      "[START_DIR]\\flutter",
      "--url",
      "https://chromium.googlesource.com/external/github.com/flutter/flutter"
    ],
    "name": "git setup"
  },
  {
    "cmd": [
      "git",
      "retry",
      "fetch",
      "origin",
      "master",
      "--recurse-submodules",
      "--progress",
      "--tags"
    ],
    "cwd": "[START_DIR]\\flutter",
    "env": {
      "PATH": "RECIPE_PACKAGE_REPO[depot_tools];<PATH>"
    },
    "infra_step": true,
    "name": "git fetch"
  },
  {
    "cmd": [
      "git",
      "checkout",
      "-f",
      "FETCH_HEAD"
    ],
    "cwd": "[START_DIR]\\flutter",
    "infra_step": true,
    "name": "git checkout"
  },
  {
    "cmd": [
      "git",
      "rev-parse",
      "HEAD"
    ],
    "cwd": "[START_DIR]\\flutter",
    "infra_step": true,
    "name": "read revision",
    "stdout": "/path/to/tmp/",
    "~followup_annotations": [
      "@@@STEP_TEXT@<br/>checked out 'deadbeef'<br/>@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@\"deadbeef\"@@@"
    ]
  },
  {
    "cmd": [
      "git",
      "clean",
      "-f",
      "-d",
      "-x"
    ],
    "cwd": "[START_DIR]\\flutter",
    "infra_step": true,
    "name": "git clean"
  },
  {
    "cmd": [
      "git",
      "submodule",
      "sync"
    ],
    "cwd": "[START_DIR]\\flutter",
    "infra_step": true,
    "name": "submodule sync"
  },
  {
    "cmd": [
      "git",
      "submodule",
      "update",
      "--init",
      "--recursive"
    ],
    "cwd": "[START_DIR]\\flutter",
    "infra_step": true,
    "name": "submodule update"
  },
  {
    "cmd": [
      "flutter.bat",
      "doctor"
    ],
    "cwd": "[START_DIR]\\flutter",
    "env": {
      "ANDROID_HOME": "[START_DIR]\\flutter\\dev\\bots\\android_tools",
      "DEPOT_TOOLS": "RECIPE_PACKAGE_REPO[depot_tools]",
      "PATH": "[START_DIR]\\flutter\\bin;[START_DIR]\\flutter\\bin\\cache\\dart-sdk\\bin;[START_DIR]\\flutter\\dev\\bots\\gradle\\gradle-4.10.2-all\\bin;<PROGRAMFILES>\\7-Zip-A\\x64;<PATH>",
      "PUB_CACHE": "[START_DIR]\\flutter\\.pub-cache"
    },
    "name": "flutter doctor"
  },
  {
    "cmd": [
      "flutter.bat",
      "update-packages"
    ],
    "cwd": "[START_DIR]\\flutter",
    "env": {
      "ANDROID_HOME": "[START_DIR]\\flutter\\dev\\bots\\android_tools",
      "DEPOT_TOOLS": "RECIPE_PACKAGE_REPO[depot_tools]",
      "PATH": "[START_DIR]\\flutter\\bin;[START_DIR]\\flutter\\bin\\cache\\dart-sdk\\bin;[START_DIR]\\flutter\\dev\\bots\\gradle\\gradle-4.10.2-all\\bin;<PROGRAMFILES>\\7-Zip-A\\x64;<PATH>",
      "PUB_CACHE": "[START_DIR]\\flutter\\.pub-cache"
    },
    "name": "download dependencies"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]\\flutter\\dev\\bots\\download_android_tools.py",
      "-t",
      "sdk"
    ],
    "env_prefixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "name": "download android tools"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::url]\\resources\\pycurl.py",
      "--url",
      "https://services.gradle.org/distributions/gradle-4.10.2-all.zip",
      "--status-json",
      "/path/to/tmp/json",
      "--outfile",
      "[START_DIR]\\flutter\\dev\\bots\\gradle-4.10.2-all.zip"
    ],
    "env_prefixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "name": "download gradle"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[build::zip]\\resources\\unzip.py"
    ],
    "env_prefixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "name": "unzip gradle",
    "stdin": "{\"output\": \"[START_DIR]\\\\flutter\\\\dev\\\\bots\\\\gradle\", \"quiet\": false, \"zip_file\": \"[START_DIR]\\\\flutter\\\\dev\\\\bots\\\\gradle-4.10.2-all.zip\"}"
  },
  {
    "cmd": [
      "cmd.exe",
      "/C",
      "[START_DIR]\\flutter\\dev\\bots\\android_tools\\sdk\\tools\\bin\\sdkmanager.bat --list"
    ],
    "env_prefixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "name": "print installed android SDK components"
  },
  {
    "cmd": [
      "dart.exe",
      "[START_DIR]\\flutter\\dev\\bots\\test.dart"
    ],
    "cwd": "[START_DIR]\\flutter",
    "env": {
      "ANDROID_HOME": "[START_DIR]\\flutter\\dev\\bots\\android_tools",
      "DEPOT_TOOLS": "RECIPE_PACKAGE_REPO[depot_tools]",
      "PATH": "[START_DIR]\\flutter\\bin;[START_DIR]\\flutter\\bin\\cache\\dart-sdk\\bin;[START_DIR]\\flutter\\dev\\bots\\gradle\\gradle-4.10.2-all\\bin;<PROGRAMFILES>\\7-Zip-A\\x64;<PATH>",
      "PUB_CACHE": "[START_DIR]\\flutter\\.pub-cache",
      "SHARD": "tests"
    },
    "name": "run test.dart for tests shard"
  },
  {
    "cmd": [
      "flutter.bat",
      "-v",
      "build",
      "apk"
    ],
    "cwd": "[START_DIR]\\flutter\\examples\\stocks",
    "env": {
      "ANDROID_HOME": "[START_DIR]\\flutter\\dev\\bots\\android_tools",
      "DEPOT_TOOLS": "RECIPE_PACKAGE_REPO[depot_tools]",
      "PATH": "[START_DIR]\\flutter\\bin;[START_DIR]\\flutter\\bin\\cache\\dart-sdk\\bin;[START_DIR]\\flutter\\dev\\bots\\gradle\\gradle-4.10.2-all\\bin;<PROGRAMFILES>\\7-Zip-A\\x64;<PATH>",
      "PUB_CACHE": "[START_DIR]\\flutter\\.pub-cache"
    },
    "name": "flutter build apk stocks"
  },
  {
    "cmd": [
      "flutter.bat",
      "-v",
      "build",
      "apk"
    ],
    "cwd": "[START_DIR]\\flutter\\examples\\flutter_gallery",
    "env": {
      "ANDROID_HOME": "[START_DIR]\\flutter\\dev\\bots\\android_tools",
      "DEPOT_TOOLS": "RECIPE_PACKAGE_REPO[depot_tools]",
      "PATH": "[START_DIR]\\flutter\\bin;[START_DIR]\\flutter\\bin\\cache\\dart-sdk\\bin;[START_DIR]\\flutter\\dev\\bots\\gradle\\gradle-4.10.2-all\\bin;<PROGRAMFILES>\\7-Zip-A\\x64;<PATH>",
      "PUB_CACHE": "[START_DIR]\\flutter\\.pub-cache"
    },
    "name": "flutter build apk flutter_gallery"
  },
  {
    "jsonResult": null,
    "name": "$result"
  }
]