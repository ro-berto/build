[
  {
    "cmd": [
      "python",
      "-u",
      "[CACHE]/builder/src/tools/mb/mb.py",
      "analyze",
      "-m",
      "tryserver.chromium.linux",
      "-b",
      "linux-rel",
      "--config-file",
      "[CACHE]/builder/src/tools/mb/mb_config.pyl",
      "--goma-dir",
      "[CACHE]/goma/client",
      "--luci-auth",
      "-v",
      "//out/Release",
      "{\"additional_compile_targets\": [], \"files\": [\"foo.cc\"], \"test_targets\": [\"base_unittests\"]}",
      "/path/to/tmp/json",
      "--json-output",
      "/path/to/tmp/json"
    ],
    "cwd": "[CACHE]/builder/src",
    "env": {
      "CHROME_HEADLESS": "1",
      "GOMA_SERVICE_ACCOUNT_JSON_FILE": "/creds/service_accounts/service-account-goma-client.json"
    },
    "name": "analyze",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"compile_targets\": [], @@@",
      "@@@STEP_LOG_LINE@json.output@  \"status\": \"No dependency\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"test_targets\": []@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@STEP_LOG_LINE@json.output[failure_summary]@{}@@@",
      "@@@STEP_LOG_END@json.output[failure_summary]@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "import sys; sys.exit(0)"
    ],
    "env": {
      "CHROME_HEADLESS": "1"
    },
    "name": "analyze_matched_exclusion",
    "~followup_annotations": [
      "@@@STEP_TEXT@Analyze disabled: matched exclusion@@@",
      "@@@STEP_LOG_LINE@excluded_files@foo.cc (regex = 'f.*')@@@",
      "@@@STEP_LOG_END@excluded_files@@@",
      "@@@STEP_LOG_LINE@analyze_details@{@@@",
      "@@@STEP_LOG_LINE@analyze_details@  \"additional_compile_targets\": [], @@@",
      "@@@STEP_LOG_LINE@analyze_details@  \"compile_targets\": [@@@",
      "@@@STEP_LOG_LINE@analyze_details@    \"base_unittests\"@@@",
      "@@@STEP_LOG_LINE@analyze_details@  ], @@@",
      "@@@STEP_LOG_LINE@analyze_details@  \"self.m.filter.compile_targets\": [@@@",
      "@@@STEP_LOG_LINE@analyze_details@    \"base_unittests\"@@@",
      "@@@STEP_LOG_LINE@analyze_details@  ], @@@",
      "@@@STEP_LOG_LINE@analyze_details@  \"self.m.filter.test_targets\": [@@@",
      "@@@STEP_LOG_LINE@analyze_details@    \"base_unittests\"@@@",
      "@@@STEP_LOG_LINE@analyze_details@  ], @@@",
      "@@@STEP_LOG_LINE@analyze_details@  \"test_targets\": [@@@",
      "@@@STEP_LOG_LINE@analyze_details@    \"base_unittests\"@@@",
      "@@@STEP_LOG_LINE@analyze_details@  ]@@@",
      "@@@STEP_LOG_LINE@analyze_details@}@@@",
      "@@@STEP_LOG_END@analyze_details@@@"
    ]
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[build::chromium]/resources/ninja_wrapper.py",
      "--ninja_info_output",
      "/path/to/tmp/json",
      "--failure_output",
      "/path/to/tmp/",
      "--no_prune_venv",
      "--",
      "RECIPE_REPO[depot_tools]/ninja",
      "-w",
      "dupbuild=err",
      "-C",
      "[CACHE]/builder/src/out/Release",
      "-j",
      "80",
      "base_unittests"
    ],
    "cwd": "[CACHE]/builder/src",
    "env": {
      "CHROME_HEADLESS": "1",
      "GOMA_USE_LOCAL": "false"
    },
    "name": "compile (with patch)"
  }
]