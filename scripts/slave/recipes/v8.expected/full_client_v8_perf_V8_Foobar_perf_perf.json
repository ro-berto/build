[
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[build::chromium]/resources/ninja_wrapper.py",
      "--ninja_info_output",
      "/path/to/tmp/json",
      "--failure_output",
      "/path/to/tmp/",
      "--no_prune_venv",
      "--",
      "RECIPE_REPO[depot_tools]/ninja",
      "-w",
      "dupbuild=err",
      "-C",
      "[CACHE]/builder/v8/out/Release",
      "-j",
      "80",
      "d8"
    ],
    "cwd": "[CACHE]/builder/v8",
    "env": {
      "CHROME_HEADLESS": "1",
      "GOMA_USE_LOCAL": "false"
    },
    "name": "build.compile",
    "~followup_annotations": [
      "@@@STEP_NEST_LEVEL@1@@@"
    ]
  }
]