[
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]/gclient.py",
      "config",
      "--spec",
      "cache_dir = None\nsolutions = [{'deps_file': 'DEPS', 'managed': True, 'name': 'skia', 'url': 'https://skia.googlesource.com/skia.git'}]"
    ],
    "name": "gclient setup"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[DEPOT_TOOLS]/gclient.py",
      "sync",
      "--nohooks",
      "--delete_unversioned_trees",
      "--force",
      "--verbose",
      "--output-json",
      "/path/to/tmp/json"
    ],
    "name": "gclient sync",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"solutions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"skia/\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"revision\": 164710@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@164710@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "gyp_skia"
    ],
    "cwd": "[SLAVE_BUILD]/skia",
    "env": {
      "GYP_DEFINES": "skia_arch_width=64 skia_gpu=0 skia_warnings_as_errors=0"
    },
    "name": "gyp_skia"
  },
  {
    "cmd": [
      "make",
      "most"
    ],
    "cwd": "[SLAVE_BUILD]/skia",
    "env": {
      "GYP_DEFINES": "skia_arch_width=64 skia_gpu=0 skia_warnings_as_errors=0"
    },
    "name": "build most"
  },
  {
    "cmd": [
      "catchsegv",
      "[SLAVE_BUILD]/skia/out/Debug/tests",
      "--verbose",
      "--tmpDir",
      "[SLAVE_BUILD]/tmp",
      "--resourcePath",
      "[SLAVE_BUILD]/skia/resources"
    ],
    "name": "tests"
  },
  {
    "cmd": [
      "catchsegv",
      "[SLAVE_BUILD]/skia/out/Debug/gm",
      "--verbose",
      "--writeChecksumBasedFilenames",
      "--mismatchPath",
      "[SLAVE_BUILD]/gm/actual/Test-Ubuntu13.10-ShuttleA-NoGPU-x86_64-Debug",
      "--missingExpectationsPath",
      "[SLAVE_BUILD]/gm/actual/Test-Ubuntu13.10-ShuttleA-NoGPU-x86_64-Debug",
      "--writeJsonSummaryPath",
      "[SLAVE_BUILD]/gm/actual/Test-Ubuntu13.10-ShuttleA-NoGPU-x86_64-Debug/actual_results.json",
      "--ignoreErrorTypes",
      "IntentionallySkipped",
      "MissingExpectations",
      "ExpectationsMismatch",
      "--resourcePath",
      "[SLAVE_BUILD]/skia/resources",
      "--deferred",
      "--pipe",
      "--replay",
      "--rtree",
      "--serialize",
      "--tileGrid"
    ],
    "name": "gm",
    "~followup_annotations": [
      "step returned non-zero exit code: 1",
      "@@@STEP_FAILURE@@@"
    ]
  }
]