      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[depot_tools]/gclient.py",
      "sync",
      "--verbose",
      "--with_branch_heads",
      "--nohooks",
      "-j8",
      "--reset",
      "--force",
      "--upstream",
      "--no-nag-max",
      "--delete_unversioned_trees",
      "--output-json",
      "/path/to/tmp/json"
    ],
    "env": {
      "PATH": "<PATH>:RECIPE_PACKAGE_REPO[depot_tools]"
    },
    "infra_step": true,
    "name": "gclient sync",
