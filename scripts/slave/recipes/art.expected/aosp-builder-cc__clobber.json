[
  {
    "cmd": [
      "RECIPE_REPO[depot_tools]/repo",
      "init",
      "-u",
      "https://android.googlesource.com/platform/manifest",
      "-b",
      "master"
    ],
    "infra_step": true,
    "name": "repo init"
  },
  {
    "cmd": [
      "RECIPE_REPO[depot_tools]/repo",
      "sync",
      "-f",
      "-c"
