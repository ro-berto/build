[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::bot_update]/resources/bot_update.py",
      "--spec-path",
      "cache_dir = '[GIT_CACHE]'\nsolutions = [{'deps_file': '.DEPS.git', 'managed': True, 'name': 'devtools-frontend', 'url': 'https://chromium.googlesource.com/devtools/devtools-frontend.git'}]",
      "--revision_mapping_file",
      "{\"got_revision\": \"devtools-frontend\"}",
      "--git-cache-dir",
      "[GIT_CACHE]",
      "--cleanup-dir",
      "[CLEANUP]/bot_update",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "devtools-frontend@HEAD"
    ],
    "cwd": "[BUILDER_CACHE]",
    "env_prefixes": {
      "PATH": [
        "None/third_party/depot_tools"
      ]
    },
    "env_suffixes": {
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"fixed_revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"devtools-frontend\": \"HEAD\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"devtools-frontend\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"repository\": \"https://fake.org/devtools-frontend.git\", @@@",
      "@@@STEP_LOG_LINE@json.output@      \"revision\": \"ad33769beb4224ffaadd7d5b01796fad5cc88cd4\"@@@",
      "@@@STEP_LOG_LINE@json.output@    }@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"devtools-frontend\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision\": \"ad33769beb4224ffaadd7d5b01796fad5cc88cd4\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"got_revision_cp\": \"refs/heads/master@{#31067}\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"devtools-frontend\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"source_manifest\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"directories\": {@@@",
      "@@@STEP_LOG_LINE@json.output@      \"devtools-frontend\": {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"git_checkout\": {@@@",
      "@@@STEP_LOG_LINE@json.output@          \"repo_url\": \"https://fake.org/devtools-frontend.git\", @@@",
      "@@@STEP_LOG_LINE@json.output@          \"revision\": \"ad33769beb4224ffaadd7d5b01796fad5cc88cd4\"@@@",
      "@@@STEP_LOG_LINE@json.output@        }@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    }, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"version\": 0@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@SET_BUILD_PROPERTY@got_revision@\"ad33769beb4224ffaadd7d5b01796fad5cc88cd4\"@@@",
      "@@@SET_BUILD_PROPERTY@got_revision_cp@\"refs/heads/master@{#31067}\"@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_REPO[depot_tools]/gclient.py",
      "runhooks"
    ],
    "cwd": "[BUILDER_CACHE]",
    "env_prefixes": {
      "PATH": [
        "[BUILDER_CACHE]/devtools-frontend/third_party/depot_tools"
      ]
    },
    "env_suffixes": {
      "PATH": [
        "RECIPE_REPO[depot_tools]"
      ]
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILDER_CACHE]/devtools-frontend/scripts/compile_frontend.py"
    ],
    "cwd": "[BUILDER_CACHE]",
    "env_prefixes": {
      "PATH": [
        "[BUILDER_CACHE]/devtools-frontend/third_party/depot_tools"
      ]
    },
    "name": "Compile Frontend"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILDER_CACHE]/devtools-frontend/scripts/lint_javascript.py"
    ],
    "cwd": "[BUILDER_CACHE]",
    "env_prefixes": {
      "PATH": [
        "[BUILDER_CACHE]/devtools-frontend/third_party/depot_tools"
      ]
    },
    "name": "Lint"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[BUILDER_CACHE]/devtools-frontend/scripts/run_tests.py"
    ],
    "cwd": "[BUILDER_CACHE]",
    "env_prefixes": {
      "PATH": [
        "[BUILDER_CACHE]/devtools-frontend/third_party/depot_tools"
      ]
    },
    "name": "Run Tests"
  },
  {
    "name": "$result"
  }
]