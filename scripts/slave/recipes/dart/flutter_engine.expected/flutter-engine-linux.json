[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "rmcontents",
      "[START_DIR]"
    ],
    "infra_step": true,
    "name": "everything"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::bot_update]/resources/bot_update.py",
      "--spec-path",
      "cache_dir = '[CACHE]/git'\nsolutions = [{'deps_file': '.DEPS.git', 'managed': True, 'name': 'src/flutter', 'url': 'https://chromium.googlesource.com/external/github.com/flutter/engine'}, {'deps_file': '.DEPS.git', 'managed': True, 'name': 'flutter', 'url': 'https://chromium.googlesource.com/external/github.com/flutter/flutter'}]\ntarget_os = ['android']",
      "--patch_root",
      "src/flutter",
      "--revision_mapping_file",
      "{}",
      "--git-cache-dir",
      "[CACHE]/git",
      "--cleanup-dir",
      "[CLEANUP]/bot_update",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "flutter@HEAD",
      "--revision",
      "src/flutter@bar",
      "--revision",
      "src/third_party/dart@foo"
    ],
    "env_prefixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"fixed_revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"flutter\": \"HEAD\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"src/flutter\": \"bar\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"src/third_party/dart\": \"foo\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"src/flutter\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"src/flutter\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"source_manifest\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"directories\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"version\": 0@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[depot_tools]/gclient.py",
      "runhooks"
    ],
    "cwd": "[START_DIR]/src/flutter",
    "env_suffixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "src/third_party/dart/tools/3xhead_flutter_hooks.sh"
    ],
    "name": "3xHEAD Flutter Hooks"
  },
  {
    "cmd": [
      "[START_DIR]/src/flutter/tools/gn"
    ],
    "name": "gn "
  },
  {
    "cmd": [
      "ninja",
      "-C",
      "[START_DIR]/src/out/host_debug"
    ],
    "name": "build host_debug"
  },
  {
    "cmd": [
      "[START_DIR]/src/flutter/tools/gn",
      "--unoptimized"
    ],
    "name": "gn --unoptimized"
  },
  {
    "cmd": [
      "ninja",
      "-C",
      "[START_DIR]/src/out/host_debug_unopt"
    ],
    "name": "build host_debug_unopt"
  },
  {
    "cmd": [
      "/bin/bash",
      "flutter/ci/analyze.sh"
    ],
    "cwd": "[START_DIR]/src",
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "analyze dart_ui"
  },
  {
    "cmd": [
      "dart",
      "[START_DIR]/src/flutter/shell/testing/observatory/test.dart",
      "[START_DIR]/src/out/host_debug/flutter_tester",
      "[START_DIR]/src/flutter/shell/testing/observatory/empty_main.dart"
    ],
    "cwd": "[START_DIR]/src",
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "test observatory and service protocol"
  },
  {
    "cmd": [
      "[START_DIR]/src/flutter/tools/gn",
      "--android"
    ],
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "gn --android"
  },
  {
    "cmd": [
      "ninja",
      "-C",
      "[START_DIR]/src/out/android_debug"
    ],
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "build android_debug"
  },
  {
    "cmd": [
      "ninja",
      "-C",
      "[START_DIR]/src/out/android_debug",
      ":dist"
    ],
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "build android_debug :dist"
  },
  {
    "cmd": [
      "[START_DIR]/src/flutter/tools/gn",
      "--android",
      "--runtime-mode=profile"
    ],
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "gn --android --runtime-mode=profile"
  },
  {
    "cmd": [
      "ninja",
      "-C",
      "[START_DIR]/src/out/android_profile"
    ],
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "build android_profile"
  },
  {
    "cmd": [
      "[START_DIR]/src/flutter/tools/gn",
      "--android",
      "--runtime-mode=release"
    ],
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "gn --android --runtime-mode=release"
  },
  {
    "cmd": [
      "ninja",
      "-C",
      "[START_DIR]/src/out/android_release"
    ],
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "build android_release"
  },
  {
    "cmd": [
      "[START_DIR]/src/flutter/tools/gn",
      "--android",
      "--android-cpu=x64"
    ],
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "gn --android --android-cpu=x64"
  },
  {
    "cmd": [
      "ninja",
      "-C",
      "[START_DIR]/src/out/android_debug_x64"
    ],
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "build android_debug_x64"
  },
  {
    "cmd": [
      "[START_DIR]/src/flutter/tools/gn",
      "--android",
      "--android-cpu=x86"
    ],
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "gn --android --android-cpu=x86"
  },
  {
    "cmd": [
      "ninja",
      "-C",
      "[START_DIR]/src/out/android_debug_x86"
    ],
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "build android_debug_x86"
  },
  {
    "cmd": [
      "[START_DIR]/flutter/bin/flutter",
      "config",
      "--no-analytics"
    ],
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "disable flutter analytics"
  },
  {
    "cmd": [
      "pub",
      "get"
    ],
    "cwd": "[START_DIR]/flutter/dev/bots",
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "pub get in dev/bots before flutter test"
  },
  {
    "cmd": [
      "[START_DIR]/flutter/bin/flutter",
      "update-packages",
      "--local-engine=host_debug",
      "--local-engine-src-path=[START_DIR]/src"
    ],
    "cwd": "[START_DIR]/flutter",
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "flutter update-packages"
  },
  {
    "cmd": [
      "dart",
      "dev/bots/test.dart",
      "--local-engine=host_debug",
      "--local-engine-src-path=[START_DIR]/src"
    ],
    "cwd": "[START_DIR]/flutter",
    "env": {
      "PATH": "[START_DIR]/src/third_party/dart/tools/sdks/dart-sdk/bin:<PATH>"
    },
    "name": "flutter test",
    "timeout": 5400
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]