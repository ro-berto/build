[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::bot_update]/resources/bot_update.py",
      "--spec",
      "cache_dir = '[CACHE]/git'\nsolutions = [{'deps_file': 'DEPS', 'managed': False, 'name': 'sdk', 'url': 'https://chromium.googlesource.com/external/github.com/dart-lang/sdk.git'}]",
      "--patch_root",
      "sdk",
      "--revision_mapping_file",
      "{}",
      "--git-cache-dir",
      "[CACHE]/git",
      "--cleanup-dir",
      "[CLEANUP]/bot_update",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "sdk@HEAD"
    ],
    "env_prefixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"fixed_revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"sdk\": \"HEAD\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"sdk\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"sdk\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[depot_tools]/gclient.py",
      "runhooks"
    ],
    "cwd": "[START_DIR]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "ASAN_SYMBOLIZER_PATH": "third_party/clang/linux/bin/llvm-symbolizer",
      "DART_USE_ASAN": "1",
      "PATH": "<PATH>:RECIPE_PACKAGE_REPO[depot_tools]"
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/sdk/tools/task_kill.py",
      "--kill_browsers=True"
    ],
    "cwd": "[START_DIR]/sdk",
    "name": "taskkill before building"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/sdk/tools/build.py",
      "-mrelease",
      "--arch=x64",
      "runtime"
    ],
    "cwd": "[START_DIR]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "ASAN_SYMBOLIZER_PATH": "third_party/clang/linux/bin/llvm-symbolizer",
      "DART_USE_ASAN": "1"
    },
    "name": "build dart"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/sdk/tools/test.py",
      "-mrelease",
      "--arch=x64",
      "--progress=line",
      "--report",
      "--time",
      "--write-debug-log",
      "--write-test-outcome-log",
      "--copy-coredumps",
      "--builder-tag=asan",
      "--timeout=240"
    ],
    "cwd": "[START_DIR]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "ASAN_SYMBOLIZER_PATH": "third_party/clang/linux/bin/llvm-symbolizer",
      "DART_USE_ASAN": "1"
    },
    "name": "vm tests"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/sdk/tools/test.py",
      "-mrelease",
      "--arch=x64",
      "--progress=line",
      "--report",
      "--time",
      "--write-debug-log",
      "--write-test-outcome-log",
      "--copy-coredumps",
      "--builder-tag=asan",
      "--timeout=240",
      "--checked",
      "--append_logs"
    ],
    "cwd": "[START_DIR]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "ASAN_SYMBOLIZER_PATH": "third_party/clang/linux/bin/llvm-symbolizer",
      "DART_USE_ASAN": "1"
    },
    "name": "checked vm tests"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/sdk/tools/task_kill.py",
      "--kill_browsers=True"
    ],
    "cwd": "[START_DIR]/sdk",
    "name": "taskkill after testing"
  },
  {
    "cmd": [
      "cat",
      ".debug.log"
    ],
    "cwd": "[START_DIR]/sdk",
    "name": "debug log"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]