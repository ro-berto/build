[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::bot_update]/resources/bot_update.py",
      "--spec-path",
      "cache_dir = '[CACHE]/git'\nsolutions = [{'deps_file': 'DEPS', 'managed': False, 'name': 'sdk', 'url': 'https://dart.googlesource.com/sdk.git'}]",
      "--patch_root",
      "sdk",
      "--revision_mapping_file",
      "{}",
      "--git-cache-dir",
      "[CACHE]/git",
      "--cleanup-dir",
      "[CLEANUP]/bot_update",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "sdk@HEAD"
    ],
    "env_prefixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"fixed_revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"sdk\": \"HEAD\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"sdk\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"sdk\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"source_manifest\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"directories\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"version\": 0@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[depot_tools]/gclient.py",
      "runhooks"
    ],
    "cwd": "[START_DIR]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "ASAN_SYMBOLIZER_PATH": "buildtools/toolchain/clang+llvm-x86_64-linux/bin/llvm-symbolizer",
      "DART_USE_ASAN": "1",
      "PATH": "<PATH>:RECIPE_PACKAGE_REPO[depot_tools]"
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/sdk/tools/task_kill.py",
      "--kill_browsers=True",
      "--kill_vsbuild=True"
    ],
    "cwd": "[START_DIR]/sdk",
    "name": "kill processes"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/sdk/tools/build.py",
      "-mrelease",
      "--arch=x64",
      "runtime"
    ],
    "cwd": "[START_DIR]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "ASAN_SYMBOLIZER_PATH": "buildtools/toolchain/clang+llvm-x86_64-linux/bin/llvm-symbolizer",
      "DART_USE_ASAN": "1"
    },
    "env_prefixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "name": "build dart"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/sdk/tools/test.py",
      "-mrelease",
      "--arch=x64",
      "--progress=line",
      "--report",
      "--time",
      "--write-debug-log",
      "--write-result-log",
      "--write-test-outcome-log",
      "--copy-coredumps",
      "--builder-tag=asan",
      "--timeout=240"
    ],
    "cwd": "[START_DIR]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "ASAN_SYMBOLIZER_PATH": "buildtools/toolchain/clang+llvm-x86_64-linux/bin/llvm-symbolizer",
      "DART_USE_ASAN": "1"
    },
    "name": "vm tests"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "copy",
      "[START_DIR]/sdk/logs/result.log",
      "/path/to/tmp/"
    ],
    "cwd": "[START_DIR]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "ASAN_SYMBOLIZER_PATH": "buildtools/toolchain/clang+llvm-x86_64-linux/bin/llvm-symbolizer",
      "DART_USE_ASAN": "1"
    },
    "infra_step": true,
    "name": "read results of vm tests",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@result.log@@@@",
      "@@@STEP_LOG_END@result.log@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "remove",
      "[START_DIR]/sdk/logs/result.log"
    ],
    "cwd": "[START_DIR]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "ASAN_SYMBOLIZER_PATH": "buildtools/toolchain/clang+llvm-x86_64-linux/bin/llvm-symbolizer",
      "DART_USE_ASAN": "1"
    },
    "infra_step": true,
    "name": "delete result.log"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/sdk/tools/test.py",
      "-mrelease",
      "--arch=x64",
      "--progress=line",
      "--report",
      "--time",
      "--write-debug-log",
      "--write-result-log",
      "--write-test-outcome-log",
      "--copy-coredumps",
      "--builder-tag=asan",
      "--timeout=240",
      "--checked",
      "--append_logs"
    ],
    "cwd": "[START_DIR]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "ASAN_SYMBOLIZER_PATH": "buildtools/toolchain/clang+llvm-x86_64-linux/bin/llvm-symbolizer",
      "DART_USE_ASAN": "1"
    },
    "name": "checked vm tests"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "copy",
      "[START_DIR]/sdk/logs/result.log",
      "/path/to/tmp/"
    ],
    "cwd": "[START_DIR]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "ASAN_SYMBOLIZER_PATH": "buildtools/toolchain/clang+llvm-x86_64-linux/bin/llvm-symbolizer",
      "DART_USE_ASAN": "1"
    },
    "infra_step": true,
    "name": "read results of checked vm tests",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@result.log@@@@",
      "@@@STEP_LOG_END@result.log@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "remove",
      "[START_DIR]/sdk/logs/result.log"
    ],
    "cwd": "[START_DIR]/sdk",
    "env": {
      "ASAN_OPTIONS": "handle_segv=0:detect_stack_use_after_return=1",
      "ASAN_SYMBOLIZER_PATH": "buildtools/toolchain/clang+llvm-x86_64-linux/bin/llvm-symbolizer",
      "DART_USE_ASAN": "1"
    },
    "infra_step": true,
    "name": "delete result.log (2)"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/sdk/tools/task_kill.py",
      "--kill_browsers=True",
      "--kill_vsbuild=True"
    ],
    "cwd": "[START_DIR]/sdk",
    "name": "kill processes (2)"
  },
  {
    "cmd": [
      "cat",
      ".debug.log"
    ],
    "cwd": "[START_DIR]/sdk",
    "name": "debug log"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]