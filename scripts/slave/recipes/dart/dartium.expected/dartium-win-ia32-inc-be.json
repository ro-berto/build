[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::bot_update]\\resources\\bot_update.py",
      "--spec-path",
      "cache_dir = '[CACHE]\\\\git'\nsolutions = [{'deps_file': 'tools/deps/dartium.deps/DEPS', 'managed': False, 'name': 'src/dart', 'url': 'https://chromium.googlesource.com/external/github.com/dart-lang/sdk.git'}]",
      "--patch_root",
      "src/dart",
      "--revision_mapping_file",
      "{}",
      "--git-cache-dir",
      "[CACHE]\\git",
      "--cleanup-dir",
      "[CLEANUP]\\bot_update",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "src/dart@12345",
      "--with_branch_heads"
    ],
    "env_prefixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "infra_step": true,
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"fixed_revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"src/dart\": \"12345\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"src/dart\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"src/dart\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"source_manifest\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"directories\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@    \"version\": 0@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]\\resources\\fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "rmtree",
      "[START_DIR]\\src\\out"
    ],
    "infra_step": true,
    "name": "clobber"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[depot_tools]\\gclient.py",
      "runhooks"
    ],
    "cwd": "[START_DIR]\\src/dart",
    "env": {
      "GYP_DEFINES": "component=static_library linux_strip_binary=1 fastbuild=1 target_arch=ia32",
      "GYP_GENERATORS": "ninja",
      "GYP_MSVS_VERSION": "2013"
    },
    "env_suffixes": {
      "PATH": [
        "RECIPE_PACKAGE_REPO[depot_tools]"
      ]
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]\\src\\dart\\tools\\task_kill.py",
      "--kill_browsers=True"
    ],
    "cwd": "[START_DIR]\\src",
    "name": "taskkill before building"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]\\src\\dart\\tools\\dartium\\build.py",
      "--mode=Release"
    ],
    "cwd": "[START_DIR]\\src",
    "env": {
      "GYP_DEFINES": "component=static_library linux_strip_binary=1 fastbuild=1 target_arch=ia32",
      "GYP_GENERATORS": "ninja"
    },
    "name": "build dartium"
  },
  {
    "allow_subannotations": true,
    "cmd": [
      "python",
      "-u",
      "[START_DIR]\\src\\dart\\tools\\dartium\\buildbot_annotated_steps.py"
    ],
    "cwd": "[START_DIR]\\src",
    "name": "annotated steps"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]\\src\\dart\\tools\\task_kill.py",
      "--kill_browsers=True"
    ],
    "cwd": "[START_DIR]\\src",
    "name": "taskkill after building"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]