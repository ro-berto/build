[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[depot_tools::bot_update]/resources/bot_update.py",
      "--spec",
      "cache_dir = '[CACHE]/git'\nsolutions = [{'custom_deps': {'dart/third_party/package-bots': 'https://dart.googlesource.com/package-bots.git', 'dart/third_party/pkg/async': 'https://github.com/dart-lang/async.git'}, 'deps_file': 'DEPS', 'managed': False, 'name': 'package-bots', 'url': 'https://dart.googlesource.com/package-bots.git'}]",
      "--patch_root",
      "package-bots",
      "--revision_mapping_file",
      "{}",
      "--git-cache-dir",
      "[CACHE]/git",
      "--cleanup-dir",
      "[CLEANUP]/bot_update",
      "--output_json",
      "/path/to/tmp/json",
      "--revision",
      "package-bots@hash_of_revision"
    ],
    "env": {
      "GIT_HTTP_LOW_SPEED_LIMIT": "1000",
      "GIT_HTTP_LOW_SPEED_TIME": "300",
      "PATH": "<PATH>:RECIPE_PACKAGE_REPO[depot_tools]"
    },
    "infra_step": true,
    "name": "bot_update",
    "~followup_annotations": [
      "@@@STEP_TEXT@Some step text@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"did_run\": true, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"fixed_revisions\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"package-bots\": \"hash_of_revision\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"manifest\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_failure\": false, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"patch_root\": \"package-bots\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"properties\": {}, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"root\": \"package-bots\", @@@",
      "@@@STEP_LOG_LINE@json.output@  \"step_text\": \"Some step text\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[depot_tools]/gclient.py",
      "runhooks"
    ],
    "cwd": "[START_DIR]/package-bots",
    "env": {
      "PATH": "<PATH>:RECIPE_PACKAGE_REPO[depot_tools]"
    },
    "name": "gclient runhooks"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/dart/tools/task_kill.py",
      "--kill_browsers=True"
    ],
    "cwd": "[START_DIR]/dart",
    "name": "taskkill before building"
  },
  {
    "allow_subannotations": true,
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/dart/third_party/package-bots/annotated_steps.py"
    ],
    "cwd": "[START_DIR]/dart",
    "name": "package_bots annotated steps"
  },
  {
    "cmd": [
      "python",
      "-u",
      "[START_DIR]/dart/tools/task_kill.py",
      "--kill_browsers=True"
    ],
    "cwd": "[START_DIR]/dart",
    "name": "taskkill after testing"
  },
  {
    "name": "$result",
    "recipe_result": null,
    "status_code": 0
  }
]