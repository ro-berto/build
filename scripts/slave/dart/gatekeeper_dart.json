{
  "comment": ["This is a configuration file for gatekeeper_ng.py",
              "Look at that for documentation on this file's format."],
  "masters": {
    "https://build.chromium.org/p/client.dart": [
      {
        "builders": {
          "*": {
            "subject_template": "Dart build %(result)s on %(builder_name)s, revision %(revision)s",
            "status_template": "New %(result) on \"%(builder_name)s\" (%(blamelist)s)",
            "respect_build_status": true,
            "close_tree": false
          }
        },
        "excluded_builders": [
          "*-dev",
          "*-stable"
        ]
      }
    ]
  }
}
