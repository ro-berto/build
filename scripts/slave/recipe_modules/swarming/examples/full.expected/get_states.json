[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_PACKAGE_REPO[build]/scripts/tools/runit.py",
      "--show-path",
      "--with-third-party-lib",
      "--",
      "python",
      "RECIPE_MODULE[build::swarming]/resources/collect_task.py",
      "--swarming-server",
      "https://chromium-swarm-dev.appspot.com",
      "--swarming-py-path",
      "[START_DIR]/swarming.client/swarming.py",
      "--json",
      "/path/to/tmp/json",
      "10000",
      "110000",
      "110100",
      "130000"
    ],
    "name": "collect tasks",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"states\": [@@@",
      "@@@STEP_LOG_LINE@json.output@    \"PENDING\"@@@",
      "@@@STEP_LOG_LINE@json.output@  ]@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  }
]