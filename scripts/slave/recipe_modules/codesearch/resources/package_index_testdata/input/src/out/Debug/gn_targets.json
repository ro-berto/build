{
  "//:test_mojom__generator": {
    "args": [
      "--use_bundled_pylibs",
      "generate",
      "-d",
      "../",
      "-I",
      "../../",
      "-o",
      "gen",
      "--bytecode_path",
      "gen/bindings",
      "--filelist={{response_file_name}}",
      "-g",
      "c++",
      "--typemap",
      "gen/mojo_bindings__type_mappings"
    ],
    "script": "//mojo/mojom_bindings_generator.py",
    "sources": ["//test.mojom"],
    "outputs": ["//out/Debug/gen/test.mojom.h"]
  },
