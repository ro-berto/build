[
  {
    "cmd": [
      "vpython",
      "-u",
      "None/out/Release/bin/run_test_name",
      "--verbose",
      "--json-results-file",
      "/path/to/tmp/json"
    ],
    "name": "test_name",
    "~followup_annotations": [
      "step returned non-zero exit code: 1",
      "@@@STEP_FAILURE@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "import sys; sys.exit(0)"
    ],
    "name": "Report test_name results",
    "~followup_annotations": [
      "@@@STEP_TEXT@<br/>failures:<br/>Test.Two<br/>@@@",
      "@@@STEP_LOG_LINE@Test.Two@:(@@@",
      "@@@STEP_LOG_END@Test.Two@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[build::test_results]/resources/upload_test_results.py",
      "--input-json",
      "{\"per_iteration_data\": [{\"Test.One\": [{\"elapsed_time_ms\": 0, \"output_snippet\": \":)\", \"status\": \"SUCCESS\"}], \"Test.Three\": [{\"elapsed_time_ms\": 0, \"output_snippet\": \"\", \"status\": \"SUCCESS\"}], \"Test.Two\": [{\"elapsed_time_ms\": 0, \"output_snippet\": \":(\", \"status\": \"FAILURE\"}]}]}",
      "--master-name",
      "test_mastername",
      "--builder-name",
      "test_buildername",
      "--build-number",
      "123",
      "--test-type",
      "test_name",
      "--chrome-revision",
      "x@{#0}",
      "--test-results-server",
      "test-results.appspot.com"
    ],
    "name": "Upload to test-results [test_name]"
  },
  {
    "cmd": [],
    "name": "details",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@details@compile_targets: ['test_name']@@@",
      "@@@STEP_LOG_LINE@details@failures: ['Test.Two']@@@",
      "@@@STEP_LOG_LINE@details@uses_local_devices: False@@@",
      "@@@STEP_LOG_END@details@@@"
    ]
  },
  {
    "name": "$result",
    "reason": "Step('test_name') failed with return_code 1",
    "recipe_result": null,
    "status_code": 1
  }
]